#include<stdio.h>
#include<string.h>
char T[1000],P[1000];
void nhap(){
    printf("Nhap T: ");
    fgets(T,sizeof(T),stdin);
    printf("Nhap P: ");
    fgets(P,sizeof(P),stdin);
}
int L(char P[],char x){
    for(int i= strlen(P)-1; i>=0;i--){
        if(P[i]==x){
            return i;
        }
    }
    return -1;
}

int tinh(char T[],char P[]){
    int m= strlen(P)-1;
    int i= m-1;
    int j=m-1;
    int min;
    while( i < strlen(T)){
        if(T[i]!=P[j]){
            if( j< 1 + L(P,T[i])){
                min = j;
            }
            else{
                min =1 + L(P,T[i]);
            }
            i= i+m-min;
            j=m-1;
        }
        else{
            if(j==0){
                return i;
            }
            i--;
            j--;
        }
    }
    return -1;
}
int main(){
    nhap();
    int x = tinh(T,P);
    if(x!=-1){
        printf("vi tri %d",x);

    }
    else    printf("k co trong chuoi");
}
