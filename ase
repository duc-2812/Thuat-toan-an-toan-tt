#include<stdio.h>
#include<math.h>

int main(){
        
        long long a=0,b=0,r;
        while(a<=0 && b<=0){
            printf("\nNhap a:");
        scanf("%lld",&a);
        printf("\nNhap b:");
        scanf("%lld",&b);
        }
        
        printf("GCD(%lld, %lld)= ",a,b);
        while(b>0){
            r=a%b;
            a=b;
            b=r;
        }
        printf(" %lld",a);
}
