#include <stdio.h>
#include <math.h>

int p, a, b, w, A[50], B[50], c[50];
int u, v, uv;
int m, t;

int nhap()
{
    printf("Nhap p: ");
    scanf("%d", &p);
    printf("Nhap w: ");
    scanf("%d", &w);
    printf("Nhap a: ");
    scanf("%d", &a);
    printf("Nhap b: ");
    scanf("%d", &b);
}

void bieudien(int x,int mang[50])
{   

    m = log(p) / log(2) + 1;
    t = (m / w) + 1;
    int tmp,j=0;
    for(int i=t-1;i>=0;i--){
        tmp=pow(2,i*w);
       mang[i]=x / tmp;
        x=x % tmp;
        printf("%d  ", mang[i]);
    }    
}
void tinh(){
   
    bieudien(a,A);
    bieudien(b,B);
   
    for (int i = 0; i <= (2 * t) - 1; i++)
    {
        c[i] = 0;
    }
    for (int i = 0; i <= t - 1; i++)
    {
        u = 0;
        for (int j = 0; j <= t - 1; j++)
        {
            uv = c[i + j] + A[i]*B[j] + u;

            u = uv / 256;
            v = uv % 256;
            c[i + j] = v;
        }
        c[i + t] = u;
    }
    for (int i = 2*t-1; i>=0; i--)
    {
        printf("\nc[%d] = %d", (2*t-1)-i, c[i]);
    }
}

int main()
{
    nhap();
    tinh();
}
