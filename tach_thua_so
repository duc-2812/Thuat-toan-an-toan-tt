#include<stdio.h>

int main(){
    int n;
    printf("\nNhap n: ");
    scanf("%d",&n);
    int tmp;
    for(int i=2;i<=n;i++){
        tmp=0;
        while( n%i==0){
            ++tmp;
            n/=i;
        }
        if(tmp){
            if(tmp>1){
                printf("%d^%d ",i,tmp);
            }
            else printf("%d",i);
            if(n>i){
                printf(" * ");
            }
        }
    }
}
