#include<stdio.h>
#include<math.h>


int main(){
        int a,b,r;
        printf("\nNhap a:");
        scanf("%d",&a);
        printf("\nNhap b:");
        scanf("%d",&b);
        printf("GCD(%d, %d)= ",a,b);
        while(b>0){
            r=a%b;
            a=b;
            b=r;
        }
        printf(" %d",a);
}
